mixin logo(data)
    +el({
        tag: 'a',
        class: 'logo',
        mod: data.mod,
        mix: data.mix,
        js: data.js,
        href: main == true ? 'none' : 'main.html',
        title: main == true ? data.title : 'Перейти на главную страницу',
    })&attributes(attributes)
        if data.secondary
            +el({
                class: 'logo__image',
                tag: 'img',
                image: 'logo-secondary.svg'
            })
        else if data.small
            +el({
                class: 'logo__image',
                tag: 'img',
                image: 'logo-small.svg'
            })
        else
            +el({
                class: 'logo__image',
                tag: 'img',
                image: 'logo-primary.svg'
            })
